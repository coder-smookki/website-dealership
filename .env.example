# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production-min-32-chars

# CORS Configuration
# Для локальной разработки:
CORS_ORIGIN=http://localhost:3000
# Для туннеля (ngrok, cloudflared и т.д.), укажите URL туннеля:
# CORS_ORIGIN=https://your-tunnel-url.ngrok.io,http://localhost:3000
# Или используйте * для разрешения всех origins (не рекомендуется для production):
# CORS_ORIGIN=*

# API Configuration
# Для локальной разработки:
VITE_API_URL=http://localhost:3001
# Для туннеля укажите URL API через туннель:
# VITE_API_URL=https://your-api-tunnel-url.ngrok.io

# Tunnel Configuration (опционально)
# URL туннеля для веб-приложения (используется для автоматической настройки)
TUNNEL_URL=

# Environment
NODE_ENV=production
